    "stylelint-config-standard": "^33.0.0",
    "stylelint-config-standard-scss": "^9.0.0",
    "stylelint-no-unsupported-browser-features": "^6.1.0",
    "stylelint-order": "^6.0.3",
    "stylelint-stylistic": "^0.4.1",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vite-plugin-checker": "^0.5.6"
