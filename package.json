{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "preinstall": "npm run check-node",
    "check-node": "node ./check-node.js",
    "dev": "vite",
    "build": "tsc && vite build",
    "build:dev": "tsc && vite build --mode development",
    "preview": "vite preview",
    "start": "npm run build:dev && concurrently --kill-others --names \"eslint,stylelint,vite:build,vite:preview\" --prefix \"[{name}]\" --prefix-colors \"bgBlue.bold,bgGrey.bold,bgYellow.bold,bgCyan.bold\" --max-processes=\"75%\" \"npm run _eslint:tswatch\" \"npm run _stylelint:csswatch\" \"npm run _vite:build\" \"npm run preview\"",
    "_vite:build": "chokidar \"./src/**/*\" -c \"npm run build:dev\"",
    "_eslint:tswatch": "chokidar \"./src/**/.ts*\" -c \"npm run _rome:lint\"",
    "_eslint:ts": "rome check \"./src\"",
    "_stylelint:csswatch": "chokidar \"./dist/assets/*.css\"  -c \"npm run _stylelint:css\"",
    "_stylelint:css": "stylelint \"./dist/assets/*.css\""
  },
  "dependencies": {
    "picocolors": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "semver": "^7.5.0"
  },
  "devDependencies": {
    "@rollup/plugin-eslint": "^9.0.3",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@vitejs/plugin-react": "^3.1.0",
    "chokidar": "^3.5.3",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^8.0.1",
    "eslint": "^8.39.0",
    "eslint-plugin-compat": "^4.1.4",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sonarjs": "^0.19.0",
    "sass": "^1.58.3",
    "stylelint": "^15.4.0",
    "stylelint-config-sass-guidelines": "^10.0.0",
    "stylelint-config-standard": "^32.0.0",
    "stylelint-no-unsupported-browser-features": "^6.1.0",
    "typescript": "^4.9.3",
    "vite": "^4.1.0"
  }
}
